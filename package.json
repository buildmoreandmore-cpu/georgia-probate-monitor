{
  "name": "georgia-probate-monitor",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postbuild": "prisma db push --accept-data-loss && prisma db seed",
    "start": "next start",
    "lint": "next lint --fix",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "vercel-build": "prisma generate && next build && prisma db push --accept-data-loss"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^5.5.2",
    "next": "14.0.4",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.40.1",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.6.2",
    "typescript": "^5"
  }
}
