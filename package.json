{
  "name": "georgia-probate-monitor",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "vercel-build": "prisma generate && (prisma migrate deploy || echo 'Migration failed, continuing build...') && next build",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "seed": "prisma db seed",
    "db:studio": "prisma studio",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^5.5.2",
    "next": "14.2.31",
    "next-safe-action": "^8.0.8",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.40.1",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.31",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
